package paging_Service;

import java.util.List;

import movieDAO.movieDAO;
import paging_DAO.pagingDAO;
import paging_DTO.pagingDTO;

public class pagingService {
    private final pagingDAO pagingDAO = new pagingDAO();
	
	public List<pagingDTO> getAllmovies {
        return pagingDAO.selectAllmovies();
	}
	
	
}
